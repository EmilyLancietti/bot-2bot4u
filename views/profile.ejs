<!doctype html>
<html>
<head>
	<title>Node Authentication</title>
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
	<style>
		body 		{ padding-top:80px; word-wrap:break-word; }
	</style>
</head>
<body>
<div class="container">

	<div class="page-header text-center">
		<h1><span class="fa fa-anchor"></span> Profile Page</h1>
		<a href="/passport/logout" class="btn btn-default btn-sm">Logout</a>
	</div>

	<div class="row">

		<div class="col-sm-6">
			<div class="well">
				<h3><span class="fa fa-user"></span> User Info</h3>

					<p>
						<strong>Email</strong>: <%= user.user.email %><br>
                        <strong>Name</strong>: <%= user.user.full_name %><br>
                        <strong>Password</strong>: <%= user.user.password %><br>
                        <strong>Token</strong>: <%= user.token.token %><br>
                        <strong>Token_type</strong>: <%= user.token.type %><br>

                    </p>

			</div>
		</div>

	</div>

</div>
</body>
</html>
